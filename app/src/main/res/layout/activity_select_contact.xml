<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_select_contact"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    tools:context="org.dalol.activitylifecycleyora.SelectContactActivity">

    <TextView
        android:id="@+id/activity_select_contact_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="dddd"
        android:background="#444444"
        android:textColor="#ffffff"
        android:padding="8dp"
        />

    <Button
        android:id="@+id/activity_select_contact_search"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Search for Contact"
        android:layout_marginBottom="8dp"
        />
    <Button
        android:id="@+id/activity_select_contactButton1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Contact1"
        android:layout_marginBottom="8dp"
        />

    <Button
        android:id="@+id/activity_select_contactButton2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Contact2"
        android:layout_marginBottom="8dp"
        />

    <Button
        android:id="@+id/activity_select_contactButton3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Contact3"
        android:layout_marginBottom="8dp"
        />

    <Button
        android:id="@+id/activity_select_contactButton4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="隔开"
        android:layout_marginBottom="8dp"
        />


</LinearLayout>
